.command
  - if(model.get("isPTY"))
    a(name= model.cid)
  .sticky
    .container
      .row-fluid
        a.showLatestOutput(href= "#"+model.cid).status.alert.alert-info.span1
        .span9
          .readonly.input.uneditable-input.span9
        a.btnRemove.btn.btn-small x
        a.btnRestart.btn.btn-small r
  .row-fluid
    .span1
    .command.output.span9(tabindex="-1")
      pre.well.result.hide(tabindex="-1")
      - if(!model.get("isPTY"))
        a(name= model.cid)
      .terminal.well.hide(tabindex="-1")
  hr
