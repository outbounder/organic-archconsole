- var id = 0

mixin renderObj(name, obj)
  table.table
    tr
      td(style="max-width: 25%")
        a.nodeBtn(href="javascript: void(0)", data-id="#{uid}#{id}", data-name= name, data-state="open") #{name}
      td.node(data-id="#{uid}#{id}", style="min-width: 75%")
        - if(typeof obj == "string")
          pre!= obj
        - else
          each value,key in obj
            - id += 1
            - var named = Array.isArray(obj)?"["+key+"]":key;
            +renderObj(named, value)
  

- var named = Array.isArray(obj)?"[]":"{}";

.renderedObject(data-id= uid)
  +renderObj(named, obj)

p.alert-success Success!

script
  $(".renderedObject[data-id=#{uid}] .nodeBtn[data-id]").click(function(e){
    var id = $(e.currentTarget).attr("data-id")
    var state = $(e.currentTarget).attr("data-state")
    if(state == "open") {
      $(".node[data-id="+id+"]").hide()
      $(e.currentTarget).attr("data-state", "closed")
    } else {
      $(".node[data-id="+id+"]").show()
      $(e.currentTarget).attr("data-state", "open")
    }
  })