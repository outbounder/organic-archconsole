{
  "Jadefy": {
    "source": "node_modules/organic-jadefy",
    "exportsGlobal": "jadefy"
  },
  "BundleCode": {
    "source": "node_modules/organic-bundlecode",
    "useCache": true,
    "debug": true,
    "uglify": true,
    "plugins": [{
      "source": "/client-plugins/ClientConfigAndTemplates"
    }]
  },
  "BundleStyle": {
    "source": "node_modules/organic-bundlestyle"
  },
  "SocketIOActions": {
    "source": "node_modules/organic-socketioActions",
    "cwd": {
      "actions": "/io-actions"
    }
  },
  "HttpReactor": {
    "source": "node_modules/organic-httpReactor",
    "reactions": {
      "root": "context/http/reactions",
      "extname": ".js",
      "indexname": "index"
    },
    "capture": {
      "type": "HttpRequest"
    },
    "startReactions": [
      "node_modules/connect/lib/middleware/cookieParser",
      { 
        "source": "node_modules/connect/lib/middleware/static", 
        "arguments": ["public/"]
      },{ 
        "source": "node_modules/connect/lib/middleware/static", 
        "arguments": ["context/ui/style/"]
      }
    ]
  },
  "Self": {
    "source": "node_modules/organic-cells/plasma/Self",
    "tissue": "archconsole",
    "surviveExceptions": true
  }
}